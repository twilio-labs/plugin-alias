{
  "name": "@twilio-labs/plugin-alias",
  "description": "Plugin to manage alias for twilio",
  "private": false,
  "version": "0.0.0-dev",
  "author": "Twilio",
  "bugs": "https://github.com/twilio-labs/plugin-alias/issues",
  "dependencies": {
    "@oclif/core": "1.9.0",
    "@oclif/plugin-version": "^1",
    "inquirer": "8.2.4",
    "jaro-winkler": "0.2.8",
    "npm": "8.14.0",
    "omelette": "0.4.17"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.10",
    "@oclif/test": "^2",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/node": "17.0.41",
    "chai": "^4.2.0",
    "eslint": "8.18.0",
    "eslint-config-oclif": "^4",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.3",
    "eslint-plugin-promise": "6.0.0",
    "globby": "^11",
    "inquirer-test": "2.0.1",
    "jest": "28.1.1",
    "mocha": "^9",
    "mock-inquirer": "1.0.3",
    "nyc": "^15.0.1",
    "semantic-release": "^19.0.3",
    "sinon": "14.0.0",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/twilio-labs/plugin-alias",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "name": "alias",
    "commands": "./src/commands",
    "bin": "twilio",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "repositoryPrefix": "<%- repo %>/blob/<%- version %>/<%- commandPath %>",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version"
    ],
    "topicSeparator": " ",
    "topics": {
      "alias": {
        "description": "Plugin to manage alias for oclif"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio-labs/plugin-alias"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "nyc": {
    "include": [],
    "exclude": [
      "./src/utilities/InquirerPrompts.js"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint --ignore-path .gitignore . && npm audit",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc --check-coverage --lines 90 --reporter=lcov --reporter=text mocha \"test/**/**.test.js\"",
    "version": "oclif-dev readme && git add README.md",
    "semantic-release": "semantic-release --branches main",
    "lint": "eslint --fix --ext .js,.jsx ."
  }
}
